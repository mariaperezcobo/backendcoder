<div>
    <div id="box1">
    <h2>hola </h2>
        {{#each products}}
            <div>
                <p>{{this.title}}</p>
                <p><b>{{this.price}}</b></p>
                <hr>
            </div>
        {{/each}}
    </div>
    
<script src="/socket.io/socket.io.js"></script>
<script>
        const socket = io()
        console.log('io conectado')

        socket.on('all', (data)=>{
            console.log(data)
            document.querySelector('#box').innerHTML += `<p>${data}</p>`
        })

    </script>


</div>